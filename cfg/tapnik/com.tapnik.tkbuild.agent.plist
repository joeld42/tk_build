     <true/>
     <key>KeepAlive</key>
     <true/>
     <key>Label</key>
     <string>com.tapnik.tkbuild.agent</string>
     <key>Nice</key>
     <integer>0</integer>
     <key>ProgramArguments</key>
     <array>
